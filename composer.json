{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/admin_toolbar": "^1.25",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/cer": "^4.0@alpha",
        "drupal/config_ignore": "^2.1",
        "drupal/config_split": "^1.0",
        "drupal/console": "^1.0.2",
        "drupal/contact_ajax": "^1.4@RC",
        "drupal/contact_block": "^1.4",
        "drupal/core": "~8.5",
        "drupal/ctools": "^3.0",
        "drupal/devel": "^1.2",
        "drupal/ds": "^3.1",
        "drupal/entity_browser": "2.0.0-alpha2",
        "drupal/field_group": "^1.0",
        "drupal/google_analytics": "^2.0",
        "drupal/honeypot": "^1.23",
        "drupal/image_effects": "^1.0",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/menu_block": "^1.5",
        "drupal/menu_link_attributes": "^1.0@beta",
        "drupal/metatag": "^1.5",
        "drupal/page_manager": "^4.0@beta",
        "drupal/panels": "^4.2",
        "drupal/paragraphs": "^1.3",
        "drupal/paragraphs_jquery_ui_accordion": "^1.2",
        "drupal/pathauto": "^1.1",
        "drupal/recaptcha": "^2.0",
        "drupal/redirect": "^1.0@beta",
        "drupal/seckit": "^1.0@alpha",
        "drupal/simple_sitemap": "^2.8",
        "drupal/site_verify": "1.x-dev#71734971a819fd537737c8d47c5a96b047b6a541",
        "drupal/smtp": "^1.0@beta",
        "drupal/stage_file_proxy": "^1.0@alpha",
        "drupal/webform": "^5.0@beta",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "~8.5"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.1.17"
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "html/core": ["type:drupal-core"],
            "html/libraries/{$name}": ["type:drupal-library"],
            "html/modules/contrib/{$name}": ["type:drupal-module"],
            "html/profiles/contrib/{$name}": ["type:drupal-profile"],
            "html/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/menu_block": {
                "Add option to make the starting level follow the active menu item (https://www.drupal.org/files/issues/2018-07-02/menu_block-follow-2756675-51.patch)": "https://www.drupal.org/files/issues/2018-07-02/menu_block-follow-2756675-51.patch"
            },
            "drupal/ds": {
                "Fix layout warning (https://www.drupal.org/project/ds/issues/2904575)": "https://www.drupal.org/files/issues/invalid_argument_in_ds_module-2904575-2.patch"
            }
        }
    }
}
